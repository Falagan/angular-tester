{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "models": [
        "src/lib/models"
      ],
      "models/*": [
        "src/lib/models/*"
      ],
      "ui": [
        "src/lib/ui"
      ],
      "ui/*": [
        "src/lib/ui/*"
      ],
      "services": [
        "src/lib/services"
      ],
      "services/*": [
        "src/lib/services/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
